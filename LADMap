		 var redIcon = L.icon({ //Marker for LADs with competitor coverage higher
			iconUrl: 'http://i.imgur.com/2vEJlTG.png', //
			iconSize:     [20, 24], // size of the icon
			iconAnchor:   [0, 0], // point of the icon which will correspond to marker's location
			popupAnchor:  [0, 0] // point from which the popup should open relative to the iconAnchor
		});
		
		var orangeIcon = L.icon({ //Marker for LADs with competitor coverage higher
			iconUrl: 'http://i.imgur.com/wnZsFBA.png', //
			iconSize:     [20, 24], // size of the icon
			iconAnchor:   [0, 0], // point of the icon which will correspond to marker's location
			popupAnchor:  [0, 0] // point from which the popup should open relative to the iconAnchor
		});
		
		var darkIcon = L.icon({ //Marker for LADs with competitor coverage higher
			iconUrl: 'http://i.imgur.com/LcjE7jl.png', //
			iconSize:     [20, 24], // size of the icon
			iconAnchor:   [0, 0], // point of the icon which will correspond to marker's location
			popupAnchor:  [0, 0] // point from which the popup should open relative to the iconAnchor
		});
		
		var midIcon = L.icon({ //Marker for LADs with competitor coverage higher
			iconUrl: 'http://i.imgur.com/sXQ8rhf.png', //
			iconSize:     [20, 24], // size of the icon
			iconAnchor:   [0, 0], // point of the icon which will correspond to marker's location
			popupAnchor:  [0, 0] // point from which the popup should open relative to the iconAnchor
		});
		
		var lightIcon = L.icon({ //Marker for LADs with competitor coverage higher
			iconUrl: 'http://i.imgur.com/xdNkjIp.png', //
			iconSize:     [20, 24], // size of the icon
			iconAnchor:   [0, 0], // point of the icon which will correspond to marker's location
			popupAnchor:  [0, 0] // point from which the popup should open relative to the iconAnchor
		});

		
		var markerClusters = L.markerClusterGroup();
		var doMarkersArray = [];
	
	for ( var i = 0; i < markers.length; i++ )
	{
		var diff = markers[i][3]
		
		var lon = markers[i][0];
		var lat = markers[i][1];
		var popupText = markers[i][2] + " has a coverage difference of " + markers[i][3] + "%";
		var markerLocation = new L.LatLng(lat, lon);
		if (diff < 0) {
		var m = L.marker( markerLocation, {icon: redIcon} )
			.bindPopup( popupText )
		} else if (diff == 0){
		var m = L.marker( markerLocation, {icon: orangeIcon} )
			.bindPopup( popupText )
		} else if (diff < 20){
		var m = L.marker( markerLocation, {icon: darkIcon} )
			.bindPopup( popupText )
		} else if (diff < 30){
		var m = L.marker( markerLocation, {icon: midIcon} )
			.bindPopup( popupText )
		} else {
		var m = L.marker( markerLocation, {icon: lightIcon} )
			.bindPopup( popupText )
		}
 		
		
			
		doMarkersArray[i] = m;	
			
		markerClusters.addLayer( m );
		}

	map.addLayer( markerClusters );

	

	</script>
